package reversiassignment;

import java.util.HashSet;
import java.util.LinkedList;
import java.util.List;
import java.util.Set;

public class MiniMax {
	
	public MiniMax(Board board, int score) {
		
	}
	
	private List<GameState> getChildren(Board board, int score) {
		List<GameState> children = new LinkedList<GameState>();
		Set<Position> adjs = board.getAdjs();
		Board b;
		for (Position p : adjs) {
			b = new Board()
		}
		return children;
	}
	
	public Set<Position> minDecision() {
		return new HashSet<Position>();
	}
	
}
